print "Truth table for NOT\n";

print "NOT false: ";
print !false;
print "\n";

print "NOT true: ";
print !true;
print "\n";

print "\nTruth table for AND\n";

print "false AND false: ";
print false & false;
print "\n";

print "false AND true: ";
print false & true;
print "\n";

print "true AND false: ";
print true & false;
print "\n";

print "true AND true: ";
print true & true;
print "\n";

